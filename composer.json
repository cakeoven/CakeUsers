{
  "name": "cakeoven/users",
  "description": "A CakePHP Plugin for users ",
  "type": "cakephp-plugin",
  "keywords": [
    "plugin",
    "cakephp"
  ],
  "homepage": "http://github.com/gmponos/CakeUsers",
  "license": "MIT",
  "authors": [
    {
      "name": "gmponos",
      "email": "gmponos@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/gmponos/CakeRM/issues"
  },
  "require": {
    "php": ">=5.3.0",
    "cakephp/cakephp-codesniffer": "1.*",
    "ext-mcrypt": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "bin": [
    "lib/Cake/Console/cake"
  ],
  "config": {
    "vendor-dir": "vendors"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}

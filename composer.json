{
  "name": "cakeoven/users",
  "description": "A CakePHP Plugin for users ",
  "type": "cakephp-plugin",
  "keywords": [
    "plugin",
    "cakephp"
  ],
  "homepage": "http://github.com/cakeoven/CakeUsers",
  "license": "MIT",
  "authors": [
    {
      "name": "gmponos",
      "email": "gmponos@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/cakeoven/cakeusers/issues"
  },
  "require": {
    "php": ">=5.3.0",
    "ext-mcrypt": "*",
    "cakephp/cakephp": "2.7.*",
    "composer/installers": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.4",
    "phpunit/phpunit": "*"
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    },
    "installer-name": "CakeUsers"
  }
}
